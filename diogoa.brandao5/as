    def next_move(self, state):
        return {
            i=0 
            n=0
            for i in range(0,100):
                if n=0:
                    "type": random.choice(["shoot"]),
                    "direction": random.choice([[1, 0], [-1, 0], [0, 1], [0, -1]]),
            
        }


if __name__ == "__main__":
    multipaint.run(MegaBot4000)